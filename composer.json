{
    "name": "toropay/plugin-sylius",
    "type": "sylius-plugin",
    "description": "ToroPay plugin for Sylius.",
    "homepage": "https://github.com/toropay/plugin-sylius",
    "license": "MIT",
    "keywords": ["payment", "toropay", "sylius-plugin"],
    "authors": [
        {
            "name": "Ishmael Doss",
            "email": "nukboon@gmail.com",
            "homepage": "https://github.com/liverbool"
        }
    ],
    "require": {
        "php": "^7.1",
        "sylius/sylius": "^1.0.4",
        "toropay/payapi-php": "1.0.x-dev",
        "toropay/payum": "1.0.x-dev"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2",
        "phpunit/phpunit": "^5.6",
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^1.0",
        "friends-of-behat/cross-container-extension": "^1.0",
        "friends-of-behat/service-container-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^1.0",
        "friends-of-behat/variadic-extension": "^1.0",
        "lakion/mink-debug-extension": "^1.2.3",
        "se/selenium-server-standalone": "^2.52",
        "symplify/easy-coding-standard": "^2.4",
        "sylius-labs/coding-standard": "^1.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Toro\\SyliusToroPayPlugin\\": "src/",
            "Tests\\Toro\\SyliusToroPayPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/app/AppKernel.php"]
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
